<h2>Here are all of the users:</h2>
<ul>
	<% @users.each do |user|%>
		<li><strong><%=link_to user.first_name, user_path(user.id) %></strong> <%=link_to "(edit)", edit_user_path(user.id) %> <%= link_to '(delete)', user_path(user.id),
            data: {:confirm => 'Are you sure?'}, :method => :delete %></li>
	<% end %>
</ul>